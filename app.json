{
  "expo": {
    "name": "Textiles El Rey Escorpión",
    "slug": "textiles-el-rey-escorpion",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAC213"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "scheme": "com.reyescorpion.ecommerce",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "userInterfaceStyle": "light",
      "supportsTablet": true,
      "bundleIdentifier": "com.reyescorpion.ecommerce",
      "infoPlist": {
        "NSAppleSignInUsageDescription": "No se recopilan datos sensibles, solo los necesarios para iniciar sesión"
      },
      "buildNumber": "5"
    },
    "android": {
      "userInterfaceStyle": "light",
      "package": "com.reyescorpion.ecommerce",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FAC213"
      }
    },
    "plugins": [
      [
        "./plugin/withDisableForcedDarkModeAndroid.js",
        {}
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "da1c6c75-4cc6-4617-b71f-d4c2e33e6117"
      }
    }
  }
}